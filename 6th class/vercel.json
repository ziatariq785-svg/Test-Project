{
  "$schema": "https://openapi.vercel.sh/vercel",
  "routes": [
    {
      "src": "/products",
      "dest": "/products.html"
    },
    {
      "src": "/contacts",
      "dest": "/contacts.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}